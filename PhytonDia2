def Divide4 (Numero): #retorno de valores
  return Numero/4

resultado=Divide4 (16)
print (resultado)

def Cuadrado (Numx,Numy):
  Cuax = Numx ** 2
  Cuay = Numy * Numy
  return Cuax , Cuay

XCuadrado , YCuadrado = Cuadrado (2,3)
print ("X al cuadrado es "+str(XCuadrado))
print ("Y al cuadrado es "+str(YCuadrado))
